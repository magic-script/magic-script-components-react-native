{
  "name": "magic-script-components-react-native",
  "version": "1.0.2",
  "description": "Magic Script Components package for React Native.",
  "main": "index.js",
  "scripts": {
    "test": "jest -i --coverage --json --outputFile=coverage/coverage.json"
  },
  "keywords": [
    "react-native",
    "magic-script",
    "ios",
    "android",
    "ar"
  ],
  "author": "Magic Leap",
  "license": "Apache-2.0",
  "dependencies": {
    "react": "16.8.6",
    "react-reconciler": "0.20.4",
    "chroma-js": "^2.1.0"
  },
  "devDependencies": {
    "babel-plugin-module-resolver": "3.2.0",
    "react-native": "0.60.5",
    "babel-jest": "24.9.0",
    "jest": "24.9.0",
    "react-test-renderer": "16.8.6"
  },
  "peerDependencies": {
    "react-native": "0.60.5"
  },
  "jest": {
    "preset": "react-native",
    "clearMocks": true,
    "setupFiles": [
      "<rootDir>/components/__mocks__/native-modules.mock.js"
    ],
    "modulePaths": [
      "components/utils"
    ],
    "collectCoverageFrom": [
      "components/**/*.{js,jsx}"
    ],
    "coverageReporters": [
      "json",
      "lcov",
      "text",
      "text-summary",
      "clover"
    ]
  }
}
